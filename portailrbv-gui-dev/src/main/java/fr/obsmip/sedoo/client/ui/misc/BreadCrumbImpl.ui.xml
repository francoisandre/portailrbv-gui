<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:misc="urn:import:fr.obsmip.sedoo.client.ui.misc"
	>
	<ui:style>
	
  
	</ui:style>
	
	<ui:with field='messageBundle'
    type='fr.obsmip.sedoo.client.message.Message' /> 
	
    <ui:with field='bundle'
    type='fr.obsmip.sedoo.client.GlobalBundle' /> 
    <g:HTMLPanel tag="span">
    <table width="100%" style="height:24px; background-color:#4A99C6;">
    <tr>
    <td><g:HorizontalPanel ui:field="contentPanel" styleName="breadCrumbPanel" /></td>
    <td align="right">
	<g:Image ui:field='minimizeImage' resource='{bundle.minimize}'  styleName='clickableImage'></g:Image>
    <g:Image ui:field='maximizeImage' resource='{bundle.maximize}'  styleName='clickableImage'></g:Image>
	</td>
    </tr>
    </table>
    
	
    </g:HTMLPanel>
	
</ui:UiBinder> 