<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.libelle {
			font-weight: bold;
		}
		
		.paragraph {
			padding-left:10px;
			padding-top :10px;
		}
		

		.detail {
    border: 1px solid #666;
  }
  
  .header {
    background: #eee;
    border-bottom: 1px solid #666;
    padding: 0.5em;
  }
	</ui:style>
	
		<ui:with field='messageBundle'
    type='fr.obsmip.sedoo.client.message.Message' />
    
     <ui:with field='bundle'
    type='fr.obsmip.sedoo.client.GlobalBundle' />  
    
    
	
	 <g:DockLayoutPanel unit='EM' >
	 
	 
	 <g:center>
    <g:HTMLPanel>
     <p class="{style.paragraph}">
     <table>
     <tr>
     <td><g:Image ui:field='englishLanguageImage' resource='{bundle.loading}'  ></g:Image></td>
     <td><g:Label text="{messageBundle.languageSwitchingMessage}" /></td>
     </tr>
     </table>
     
    </p>
    </g:HTMLPanel>
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder> 


