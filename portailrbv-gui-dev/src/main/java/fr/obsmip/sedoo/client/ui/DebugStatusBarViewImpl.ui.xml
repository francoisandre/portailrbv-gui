<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:l="urn:import:fr.obsmip.sedoo.client.ui">
	<ui:style>
		.libelle {
			font-weight: bold;
		}
		
		
		.toolbar {
			padding: 2px;
		}
		
		.paragraph {
			padding-right:5px;
			padding-top :2px;
			padding-bottom :2px;
			text-align: right;
		}
 
	</ui:style>
	  <ui:with field='bundle'
    type='fr.obsmip.sedoo.client.GlobalBundle' /> 
	<g:DockLayoutPanel unit='PX'>
	
	<g:north size="35">
	<l:StatusBarViewImpl ui:field="statusBarView"></l:StatusBarViewImpl>
	</g:north>
	
	<g:south size="35">
	<g:HorizontalPanel styleName="{style.toolbar}">
	<g:Button ui:field="connectAdminButton" text="Se connecter Administrateur" />
	</g:HorizontalPanel>
	</g:south>
	
	</g:DockLayoutPanel>
    
</ui:UiBinder> 


