<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
  
	</ui:style>
	
	<ui:with field='messageBundle'
    type='fr.obsmip.sedoo.client.message.Message' /> 
	
    <ui:with field='bundle'
    type='fr.obsmip.sedoo.client.GlobalBundle' /> 
	<g:HTMLPanel tag="span">
	 <table class='table' width="100%">
    <col width="10%" />
  <col width="80%" />
    <tbody class="tableBody">
    	<tr><td colspan="3" align="center"><g:Label text="{messageBundle.mapSelectorNorthLatitude}" /> <g:TextBox ui:field='northBoundLatitude' visibleLength="10"/><g:Label ui:field="northBoundLatitudeDisplay" /></td></tr>
    	<tr ><td><g:Label text="{messageBundle.mapSelectorWestLongitude}" /><g:TextBox ui:field='westBoundLongitude' visibleLength="10"/><g:Label ui:field="westBoundLongitudeDisplay" /></td><td align="center"><g:VerticalPanel ui:field="contentPanel"> </g:VerticalPanel></td><td><g:Label text="{messageBundle.mapSelectorEastLongitude}" /><g:TextBox ui:field='eastBoundLongitude' visibleLength="10"/> <g:Label ui:field="eastBoundLongitudeDisplay" /></td></tr>
        <tr ><td colspan="3" align="center"><g:Label text="{messageBundle.mapSelectorSouthLatitude}" /><g:TextBox ui:field='southBoundLatitude' visibleLength="10"/><g:Label ui:field="southBoundLatitudeDisplay" /></td></tr>
    </tbody>
    </table>
	</g:HTMLPanel>
</ui:UiBinder> 