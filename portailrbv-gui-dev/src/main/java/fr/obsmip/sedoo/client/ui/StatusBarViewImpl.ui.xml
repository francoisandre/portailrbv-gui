<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
		
		
 
	</ui:style>
	  <ui:with field='bundle'
    type='fr.obsmip.sedoo.client.GlobalBundle' /> 
	<g:DockLayoutPanel unit='PX'>
	<g:center>
	<g:HTMLPanel height="35px">
	<table width="100%" class="statusBar">
	<tr >
	<td><g:Image resource='{bundle.loading}' ui:field="loading" visible="false"  ></g:Image>
	<span ui:field='message'/></td>
	<td style="text-align:right">
	<p >
      <span><span class='libelle'>Version : </span><span ui:field='version'/></span>
    </p>
	</td>
	</tr>
	</table>
	</g:HTMLPanel>
	</g:center>
	</g:DockLayoutPanel>
    
</ui:UiBinder> 


