<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:l="urn:import:fr.obsmip.sedoo.client.ui.misc"
	xmlns:tabs="urn:import:fr.obsmip.sedoo.client.ui.tabs.display">
	<ui:style>
		
		.paragraph {
			padding-left:10px;
			padding-top :10px;
		}
		
		.end {
			padding-bottom :10px;
		}
		
  
   .tabPanelExample1
        {
            margin-left: 10px;
            margin-right: 10px;
        }
        
     
@external .gwt-TabLayoutPanelContent;
 
.gwt-TabLayoutPanelContent
{
overflow: auto;     
}
 
  
  
	</ui:style>
	
	<ui:with field='messageBundle'
    type='fr.obsmip.sedoo.client.message.Message' /> 
    
	 <g:DockLayoutPanel unit='PX' styleName='sectionBorder'>
	 
	 <g:north size='38'>
      <g:HTMLPanel styleName='sectionHeader'>
      <div > <g:Label text="{messageBundle.metadataEditingTitle}" /></div>
      </g:HTMLPanel>
    </g:north>
    
   
	 <g:center >
	 
	 <g:TabLayoutPanel addStyleNames="{style.tabPanelExample1}" ui:field="tabPanel" barUnit="PX" barHeight="40" >
		<g:tab>
			<g:header>
				Identification
			</g:header>
			<tabs:IdentificationTab ui:field="identificationTab"></tabs:IdentificationTab>
		</g:tab>
		<!-- 
		<g:tab>
			<g:header>
				GÃ©ographical Location
			</g:header>
			<tabs:GeographicalLocationTab/>
		</g:tab>
		 -->
	</g:TabLayoutPanel>
	 
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder> 


